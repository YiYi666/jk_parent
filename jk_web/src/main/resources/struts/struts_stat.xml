<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>


    <package name="stat" namespace="/stat" extends="default">
        <action name="statChartAction_*" class="statChartAction" method="{1}">
            <result name="factorysale">/WEB-INF/pages/stat/chart/factorysale.jsp</result>
            <result name="productsale">/WEB-INF/pages/stat/chart/productsale.jsp</result>
            <result name="onlineinfo">/WEB-INF/pages/stat/chart/onlineinfo.jsp</result>
        </action>
    </package>

</struts>
