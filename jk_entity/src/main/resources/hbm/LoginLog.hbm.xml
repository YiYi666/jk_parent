<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="top.greathead.jk.entity.LoginLog" table="LOGIN_LOG_P" schema="JK" dynamic-update="true" dynamic-insert="true">
        <id name="id" column="LOGIN_LOG_ID">
            <generator class="uuid"/>
        </id>
        <property name="loginName" column="LOGIN_NAME"/>
        <property name="ipAddress" column="IP_ADDRESS"/>
        <property name="loginTime" column="LOGIN_TIME"/>
    </class>
</hibernate-mapping>